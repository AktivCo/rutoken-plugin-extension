{
  "name": "Adapter Rutoken Plugin",
  "author": "CJSC Aktiv-Soft",
  "version": "RUTOKEN_PLUGIN_DOT_SEPARATED_VERSION",
  "description": "Позволяет использовать Рутокен Плагин",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [{
      "matches": [ "<all_urls>" ],
      "js": ["content.js"],
      "all_frames": true,
      "run_at": "document_start"
  }],
  "web_accessible_resources": [
      "webpage.js"
  ],
  "permissions": [
      "nativeMessaging"
  ]
}
